{
  "$schema": "http://json.schemastore.org/template",
  "author": "HavenDV",
  "classifications": [ "Gtk" ],
  "identity": "Gtk.GtkApp",
  "name": "Gtk .NET Application",
  "description": "A project for creating an Gtk .NET application",
  "shortName": "gtk",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "GtkApp1",
  "preferNameDirectory": true,
  "primaryOutputs": [
    { "path": "GtkApp1.csproj" }
  ],
  "symbols": {
    "packageName": {
      "type": "parameter",
      "description": "Overrides the package name in the gtk-manifest.xml",
      "datatype": "string",
      "replaces": "com.companyname.GtkApp1"
    }
  },
  "defaultName": "GtkApp1"
}